#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int v[4][4];
int main (){
	for(int i=0; i<=3; i++){
		for (int j=0; j<=3; j++){
		printf("Digite os valores da matriz: ");
		scanf("%d", &v[i][j]);
		}	}
		printf("\n\nValores da diagonal pricipal: ");
		for (int j=0; j<=3; j++){
		printf("%d   ", v[j][j]);
		}
}
